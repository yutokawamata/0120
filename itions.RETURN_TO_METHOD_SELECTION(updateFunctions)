+import React from 'react';
+import { StateCheckers } from '../utils/stateManager';
+import styles from '../styles/components/Title.module.css';
+
+// ã‚¿ã‚¤ãƒˆãƒ«ã®å®šç¾©ã‚’ä¸€ç®‡æ‰€ã«ã¾ã¨ã‚ã‚‹
+const TITLES = {
+  MAIN: "ã‹ã‚“ã˜ãƒãƒ£ãƒ¬ãƒ³ã‚¸",
+  SELECT_KANJI: "æ¼¢å­—ã‚’é¸ã¶",
+  SELECT_REPETITION: "åå¾©å›æ•°é¸æŠ",
+  SELECT_CONFIRMATION: "ãŸã—ã‹ã‚ã‚ˆã†å›æ•°é¸æŠ",
+  COMPLETE: "ãŒã‚“ã°ã£ãŸã­ï¼"
+};
+
+/**
+ * ç¾åœ¨ã®çŠ¶æ…‹ã«å¿œã˜ãŸã‚¿ã‚¤ãƒˆãƒ«ã‚’å–å¾—ã™ã‚‹
+ * @param {Object} state - ã‚¢ãƒ—ãƒªã‚±ãƒ¼ã‚·ãƒ§ãƒ³ã®çŠ¶æ…‹
+ * @returns {string|null} - è¡¨ç¤ºã™ã‚‹ã‚¿ã‚¤ãƒˆãƒ«
+ */
+const getTitleForState = (state) => {
+  // ãŸã—ã‹ã‚ã‚ˆã†ï¼ã®å­¦ç¿’çµ‚äº†å¾Œ
+  if (StateCheckers.isEndScreen(state)) {
+    return TITLES.COMPLETE;
+  }
+
+  // æœ€åˆã®ç”»é¢ã¯ã‚¿ã‚¤ãƒˆãƒ«ã‚’è¡¨ç¤º
+  if (StateCheckers.isMainMenuScreen(state)) {
+    return TITLES.MAIN;
+  }
+
+  // å­¦ç¿’ä¸­ã¯ä½•ã‚‚è¡¨ç¤ºã—ãªã„
+  if (StateCheckers.isTraining(state)) {
+    return null;
+  }
+
+  // çŠ¶æ…‹ã«å¿œã˜ãŸã‚¿ã‚¤ãƒˆãƒ«ã‚’è¿”ã™
+  if (StateCheckers.isLevelSelection(state)) {
+    return TITLES.SELECT_KANJI;
+  }
+
+  //æ¼¢å­—ä¸€è¦§ç”»é¢ã¯ä½•ã‚‚è¡¨ç¤ºã—ãªã„
+  if (StateCheckers.isKanjiSelection(state)) {
+    return null;
+  } 
+
+  if (StateCheckers.isRepetitionSelection(state)) {
+    return TITLES.SELECT_REPETITION;
+  }
+
+  if (StateCheckers.isConfirmationSelection(state)) {
+    return TITLES.SELECT_CONFIRMATION;
+  }
+
+  return null;
+};
+
+/**
+ * ã‚¿ã‚¤ãƒˆãƒ«ã‚³ãƒ³ãƒãƒ¼ãƒãƒ³ãƒˆ
+ * ç¾åœ¨ã®ç”»é¢çŠ¶æ…‹ã«å¿œã˜ã¦é©åˆ‡ãªã‚¿ã‚¤ãƒˆãƒ«ã‚’è¡¨ç¤ºã™ã‚‹
+ * @param {Object} props
+ * @param {Object} props.state - ã‚¢ãƒ—ãƒªã‚±ãƒ¼ã‚·ãƒ§ãƒ³ã®çŠ¶æ…‹
+ */
+export const Title = ({ state }) => {
+  const title = getTitleForState(state);
+
+  if (!title) {
+    return null;
+  }
+
+  return (
+    <div className={styles.title}>
+      <h3>{title}</h3>
+    </div>
+  );
+};
   
